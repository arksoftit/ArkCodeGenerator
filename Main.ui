/* --- ESTADO DESHABILITADO REFORZADO --- */

QLineEdit:disabled, 
QComboBox:disabled, 
QDateEdit:disabled, 
QTimeEdit:disabled {
    background-color: #e1e1e1;
    color: #808080;
    border: 2px solid #b0b0b0;
}

